@article{asaro_material_1985,
	title = {Material modelling and failure modes in metal plasticity},
	volume = {4},
	issn = {0167-6636},
	doi = {10.1016/0167-6636(85)90032-8},
	abstract = {This paper is concerned with introducing some aspects of material modelling for metal plasticity which have particular relevance to the description of localized deformation and failure modes. These aspects of metal constitutive behavior are discussed in more detail in various papers in this volume dealing with material modelling and localized plastic flow and failure. Stress and strain state, and path dependence of strain hardening is discussed from the standpoints of existing phenomenological constitutive theories, experiment, and physically based constitutive models. In addition, a brief discussion is given of the experimental phenomenology of localized deformation as observed over a range of physical size scales. As summarized, the observations indicate that localized for deformation is, in many cases, caused by micro-fractures that occur during plastic deformation but can also be the natural outcome of large strain deformation process in homogeneous, damage free strain hardening materials. Some effects of strain rate sensitivity are discussed as regards path dependent stress-strain response and localized plastic flow.},
