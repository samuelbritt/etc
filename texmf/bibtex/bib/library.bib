	pages = {443--477}
},

@article{rees_sheet_1996,
	title = {Sheet orientation and forming limits under diffuse necking},
	volume = {20},
	issn = {{0307-904X}},
	doi = {10.1016/0307-904X(96)00010-8},
	abstract = {This paper examines the influence of orientation ([theta]) between the principal stress axes and the material axes when modelling the forming limits for a rolled sheet. Stress and strain transformation equations are coupled with orthotropic plasticity theory1 when the stress ratio remains constant. Three limit strains, two normal and one shear, appear in the material axes at diffuse instability. These strains define a forming limit diagram {(FLD)} which rotates, with increasing [theta], about its pole. Shear strains reach a maximum for [theta] = 45 {\textdegree} but are absent for [theta] = 0 and 90 {\textdegree}. A simpler plane presentation of the {FLD} appears in axes of major and minor principal plastic strains. This shows that the sensitivity of the {FLD} to orientation depends upon the r-values. Examples are given of 1. (i) a {CR} steel whose r-values exceed unity but where r1 {\textasciitilde}- r2, i.e., where only a slight deviation exists between the principal axes of stress and strain and2. (ii) a 6000 aluminium alloy whose r values are different and less than unity. The observations made provide a mathematical model of the experimental {Keeler-Goodwin} {FLD2,3.} The use of thickness strain for one axis of the {FLD} is also considered for when ultrasonic thickness monitoring is more convenient than surface strain measurement. Fracture strain data from bulge forming, punch indentation, and Ericksen tests fall around the limit lines predicted by a diffuse instability model in the region of most interest to users.},
	number = {8},
	journal = {Applied Mathematical Modelling},
	author = {D. W. A. Rees},
	month = aug,
	year = {1996},
	pages = {624--635}
},

