@book{kocks_polycrystal_1994,
	title = {Polycrystal models to fit experiments},
	isbn = {{LA-UR--94-2141;} {CONF-9409173--1;} Other: {ON:} {DE94014419}
United {StatesOther:} {ON:} {DE94014419Tue} Feb 12 20:53:04 {EST} {2008OSTI} as {DE94014419;} Paper copy available at {OSTI:} phone, 865-576-8401, or email, {reports@adonis.osti.govLANL;} {SCA:} 360103; {PA:} {EDB-94:109540;} {ERA-19:025024;} {NTS-94:022315;} {SN:} {94001041404English}},
	abstract = {Two problems in the modeling of polycrystal plasticity are addressed in which some parameter can best be determined by matching with experiment, although the principles of the underlying mechanisms are presumed known. One of these problems is the transition from ``full constraints`` {(FC)} to ``relaxed constraints`` {(RC)} with increasing flatness of the grains. Observed qualitative transitions in texture with strain, such as a transient orthotropic symmetry in torsion textures, can help identify the rate at which the {FC-to-RC} transition takes place. The second problem is that of the material dependence of deformation textures among the {FCC} metals which, it is argued, can only be due to a change in deformation modes, i.e., in the shape of the single-crystal yield surface. A heuristic assumption of an increasing importance of (111){\textless}211{\textgreater}-slip as the stacking-fault energy decreases explains the qualitative trend. The quantitative parameter needed has been determined for copper from a match of prediction and experiment over a range of strains.},
	author = {U. F Kocks and C. T Necker},
	year = {1994}
},

