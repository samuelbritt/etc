         \renewcommand*{\si@unt@nonlatin}{%
           \catcode176=\active
           \catcode181=\active
           \catcode197=\active
           \si@unt@sym{176}{\si@sym@degree}%
           \si@unt@sym{181}{\si@sym@mu}%
           \si@unt@sym{197}{\si@sym@ringA}}%
       \fi}}
    {}}
