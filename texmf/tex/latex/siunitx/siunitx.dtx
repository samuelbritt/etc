    \renewcommand*{\si@unt@stackb}{}%
    \renewcommand*{\si@unt@spacestacka}{}%
    \renewcommand*{\si@unt@spacestackb}{}%
    \renewcommand*{\si@unt@lastadda}{space}%
    \renewcommand*{\si@unt@lastaddb}{space}}
