    \let^\si@out@sp
    \let_\si@out@sb
    \let\textsuperscript\si@out@sp
    \let\textsubscript\si@out@sb
    \catcode`\-=\active\relax
    \let-\si@out@minus
    #1}
  \gdef\si@out@maths#1{%
    \let^\sp
    \let_\sb
    \let\textsuperscript\sp
    \let\textsubscript\sb
    $\si@fam@maths{#1}$}
