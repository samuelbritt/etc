\newcommand*{\si@symbol}[1]{%
  \expandafter\protected\expandafter\def
    \csname si@sym@#1\endcsname{%
    \ifmmode
      \expandafter\csname si@maths#1\expandafter\endcsname
    \else
      \expandafter\csname si@text#1\expandafter\endcsname
    \fi}}
