        \expandafter\si@fam@set
      \fi
      \text{%
        \si@colourcmd{\si@colour}%
        \si@fam@italic\si@fam@bold\si@fam@text
%    \end{macrocode}
%  The correct mode is selected, and the input is handed off for
%  typesetting.
%    \begin{macrocode}
        \ifsi@textmode
          \expandafter\si@out@text
        \else
          \expandafter\si@out@maths
        \fi
        {\scantokens{#1}}}%
    \endgroup
    \check@mathfonts}
%    \end{macrocode}
